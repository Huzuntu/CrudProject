<% for(var i = 0; i < users.users.length; i++) { %>
  <tr>
      <td><%= ((users.page - 1) * 10) + i + 1 %></td>
      <td><%= users.users[i].name %></td>
      <td><%= users.users[i].email %></td>
      <td><%= users.users[i].gender %></td>
      <td><%= users.users[i].status %></td>
      <td>
          <a href="/update-user?id=<%= users.users[i]._id%>" class="btn border-shadow update">
              <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
          </a>
          <a class="btn border-shadow delete" data-id=<%= users.users[i]._id%> >
              <span class="text-gradient"><i class="fas fa-times"></i></span>
          </a>
      </td>
  </tr>
<% } %>


